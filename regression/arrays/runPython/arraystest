  b = zeros((3, 2, ), int32)
  res = zeros((3, 2, ), int32)
  savespace(a)
  savespace(b)
  savespace(res)
  res = ArrayTest.ArrayOps.matrixMultiply(a,b,res)
  if (isinstance(intArray, ArrayType) and
      ArrayTest.ArrayOps.checkMatrixMultiply(a,b,res)):
