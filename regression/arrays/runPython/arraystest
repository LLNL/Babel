if __name__ == '__main__':
  import sys
  tracker = RegOut.RegOut()
  tracker.setExpectations(-1)
  try:
    driver(tracker)
  except:
    (type, value, trace) = sys.exc_info()
    tracker.forceFailure();
    import traceback
    traceback.print_exception(type, value, trace)

  tracker.close()
0
